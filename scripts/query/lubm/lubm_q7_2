PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX ub: <http://swat.cse.lehigh.edu/onto/univ-bench.owl#>

SELECT ?X ?Y ?Z WHERE {
	?Y rdf:type ub:FullProfessor <-
	?Y ub:teacherOf ?Z .
	?Z rdf:type ub:Course .
	?X ub:takesCourse ?Z <-
	?X ub:advisor    ?Y .
	?X rdf:type ub:UndergraduateStudent .
}
